   "source": [
    "p=re.compile('[a-z]+')\n",
    "if p.match(\"python\"):\n",
    "  print('Match')\n",
    "\n",
    "\n",
    "if not re.match('a$',\"ca\"):\n",
    "     print(\"Match\")\n",
    "else:\n",
